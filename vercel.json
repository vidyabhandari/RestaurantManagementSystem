{
    "projects": [
      {
        "src": "frontend/package.json",
        "use": "@vercel/static-build"
      },
      {
        "src": "backend/server.js",
        "use": "@vercel/node"
      }
    ],
    "rewrites": [
      {
        "source": "/api/(.*)",
        "destination": "/backend/$1"
      }
    ]
  }